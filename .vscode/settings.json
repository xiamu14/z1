{
  "editor.tabSize": 2,
  "editor.indentSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "tailwindCSS.experimental.classRegex": [
    ["([\"'`][^\"'`]*.*?[\"'`])", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "tailwindCSS.classAttributes": ["class", "className", ".*ClassName"],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/flutter/ephemeral": true,
    "**/Flutter/ephemeral": true,
    "**/.symlinks": true,
    "**/.plugin_symlinks": true,
    "**/*.blob": true,
    "**/*.csproj": true,
    ".vscode": true,
    ".next": true,
    "node_modules": true,
    "next-env.d.ts": true
  },
  "hide-files.files": [".vscode", ".next", "node_modules", "next-env.d.ts"],
  "cSpell.words": ["Contentlayer", "Pressable", "remixicon", "shikijs"]
}
